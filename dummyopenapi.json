  "paths": {
    "/accounts/{accountId}/connected-fields/tab-groups": {
      "get": {
        "operationId": "ESign Tabs Discovery",
        "summary": "Fetches all tabs associated with the given account",
        "parameters": [
          {
            "name": "accountId",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "appId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "connectorId",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "locale",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "unifiedModelCategory",
            "required": false,
            "in": "query",
            "schema": {
              "enum": [
                "UNIFIEDMODELCATEGORY_UNKNOWN",
                "UNIFIEDMODELCATEGORY_NONE",
                "UNIFIEDMODELCATEGORY_OOTB",
                "UNIFIEDMODELCATEGORY_CUSTOM",
                "UNIFIEDMODELCATEGORY_CONNECTED",
                "UNIFIED_MODEL_CATEGORY_CONNECTED"
              ],
              "type": "string"
            }
          },
          {
            "name": "sops",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "write",
                  "read",
                  "verify"
                ]
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/TabInfo"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDTO"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDTO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDTO"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - Authentication is required",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDTO"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDTO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponseDTO"
                }
              }
            }
          }
        },
        "tags": [
          "Tab Info"
        ]
      }
    }
  },
